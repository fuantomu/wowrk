function equipItem(character, type, slot, itemid)
	local pItem = {}
	if (type == "weapon") then
		pItem = getWeaponData(tostring(itemid))
		characterData[character][slot] = pItem
		characterData[character][slot]["slot"] = slot
	else
		pItem = getArmorData(tostring(itemid))
		characterData[character][slot] = pItem
		characterData[character][slot]["slot"] = slot
	end
end



